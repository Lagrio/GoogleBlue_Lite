<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/lib/com.tencent.mobileqq"
    android:orientation="vertical"
    android:id="@*com.tencent.mobileqq:id/b8b"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@*com.tencent.mobileqq:id/j2c"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.tencent.widget.immersive.ImmersiveTitleBar2
            android:id="@*com.tencent.mobileqq:id/jq6"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_alignParentTop="true" />
        <RelativeLayout
            android:id="@*com.tencent.mobileqq:id/idz"
            android:layout_width="match_parent"
            android:layout_height="@*com.tencent.mobileqq:dimen/title_bar_height"
            android:layout_below="@*com.tencent.mobileqq:id/jq6">
            <TextView
                android:textSize="24sp"
                android:textStyle="bold"
                android:textColor="#FFFFFFFF"
                android:ellipsize="end"
                android:gravity="center"
                android:id="@*com.tencent.mobileqq:id/ivTitleName"
                android:text="@*com.tencent.mobileqq:string/hvo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="60dp"
                android:layout_marginRight="100dp"
                android:singleLine="true"
                android:drawablePadding="5dp"
                android:layout_centerVertical="true"/>
            <ImageView
                android:id="@*com.tencent.mobileqq:id/ivTitleBtnRightImage"
                android:background="@*com.tencent.mobileqq:drawable/top_button_right_selector"
                android:paddingLeft="6dp"
                android:paddingTop="3dp"
                android:paddingRight="6dp"
                android:paddingBottom="3dp"
                android:layout_width="36dp"
                android:layout_height="30dp"
                android:layout_marginRight="10dp"
                android:src="@*com.tencent.mobileqq:drawable/tab_contacts_icon"
                android:scaleType="center"
                android:includeFontPadding="false"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true" />
        </RelativeLayout>
    </RelativeLayout>
    <include
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        layout="@*com.tencent.mobileqq:layout/hg" />
    <com.tencent.mobileqq.activity.contacts.pullrefresh.CommonRefreshLayout
        android:id="@*com.tencent.mobileqq:id/j8z"
        android:background="@*com.tencent.mobileqq:drawable/bg_texture_theme_version2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <include
            android:id="@*com.tencent.mobileqq:id/j8x"
            layout="@*com.tencent.mobileqq:layout/ib" />
        <FrameLayout
            android:id="@*com.tencent.mobileqq:id/j8y"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:id="@*com.tencent.mobileqq:id/b8c"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@*com.tencent.mobileqq:drawable/bg_texture_theme_version2"
                android:scaleType="centerCrop" />
            <com.tencent.mobileqq.activity.contacts.base.HeaderScrollView
                android:orientation="vertical"
                android:id="@*com.tencent.mobileqq:id/b8a"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <include
                    android:id="@*com.tencent.mobileqq:id/b7z"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    layout="@*com.tencent.mobileqq:layout/i5" />
                <FrameLayout
                    android:background="@*com.tencent.mobileqq:drawable/jh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.tencent.mobileqq.activity.contacts.base.tabs.SimpleSlidingIndicator
                        android:textSize="16dp"
                        android:id="@*com.tencent.mobileqq:id/ixv"
                        android:layout_width="match_parent"
                        android:layout_height="56dp"
                        app:ca="@*com.tencent.mobileqq:color/hd"
                        app:indicatorAlpha="25"
                        app:indicatorColor="@*com.tencent.mobileqq:color/hd"
                        app:indicatorMarginLeftRight="14dp"
                        app:indicatorMarginTopBottom="12dp"
                        app:indicatorRoundRectX="16dp"
                        app:indicatorRoundRectY="16dp"
                        app:tabContainerPaddingLeftRight="16dp"
                        app:tabGap="2dp"
                        app:tabPaddingLeftRight="14dp"
                        app:uncheckTextColor="@*com.tencent.mobileqq:color/skin_gray2_theme_version2" />
                </FrameLayout>
                <com.tencent.mobileqq.activity.contacts.base.tabs.ContactsViewPager
                    android:id="@*com.tencent.mobileqq:id/b8d"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </com.tencent.mobileqq.activity.contacts.base.HeaderScrollView>
        </FrameLayout>
    </com.tencent.mobileqq.activity.contacts.pullrefresh.CommonRefreshLayout>
</LinearLayout>
