<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@*com.tencent.mobileqq:drawable/qq_setting_me_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.tencent.mobileqq.widget.RandomCoverView
        android:id="@*com.tencent.mobileqq:id/jt4"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:scaleType="fitCenter"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" />
    <ImageView
        android:id="@*com.tencent.mobileqq:id/ac5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@*com.tencent.mobileqq:id/jt4"
        android:layout_alignParentLeft="true" />
    <ImageView
        android:id="@*com.tencent.mobileqq:id/kis"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:scaleType="centerCrop"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true" />
    <ImageView
        android:id="@*com.tencent.mobileqq:id/a81"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop" />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@*com.tencent.mobileqq:drawable/qq_setting_me_bg"
        android:scaleType="centerCrop" />
</RelativeLayout>
